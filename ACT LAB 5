def classify_age(age):
    if age < 0:
        print("Invalid age entered.")
    elif age <= 12:
        print("You are a child.")
    elif age <= 19:
        ("You are a Teen.")
    elif age <= 64:
        print("You are an Adult")
    else:
        print("You are a senior.")
        
while True:
    age = int(input("Enter your age: "))
    classify_age(age)
